#include "/home/andre/.colors/high"


! Hi-DPI
Xcursor.size: 28
Xft.dpi: 116
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb


! Rofi
rofi.modi: window,run,drun
rofi.color-enabled: true
rofi.terminal: termite
rofi.show-icons: true
rofi.lines: 5

! Window	    backgroud  border   separator
rofi.color-window: #263238,   #263238, #263238
rofi.color-normal: #263238, #d8dee8, #263238, #263238, #00bcd4
rofi.color-active: #263238, #b48ead, #263238, #263238, #93e5cc
rofi.color-urgent: #263238, #ebcb8b, #263238, #263238, #ebcb8b

! Hide scroll
rofi.hide-scrollbar: true
